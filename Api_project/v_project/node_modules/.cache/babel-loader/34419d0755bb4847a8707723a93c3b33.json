contribution: {\n          project: 0,\n          case: 0,\n          api: 0,\n          monitor: 0\n        }\n      },\n      real_time_datas: {\n        ApiShop_count: 0,\n        UnReadNews_count: 0,\n        RunCase_count: 0,\n        Import_count: 0\n      }\n    };\n  },\n  methods: {},\n  components: {\n    Menu: Menu\n  },\n  mounted: function mounted() {\n    var _this = this;\n\n    axios(this.baseUrl + '/get_tj_datas/').then(function (res) {\n      _this.tj_datas = res.data;\n    });\n    axios.get(this.baseUrl + '/get_real_time_datas/').then(function (res) {\n      _this.real_time_datas = res.data;\n    });\n    setInterval(function () {\n      axios.get(_this.baseUrl + '/get_real_time_datas/').then(function (res) {\n        _this.real_time_datas = res.data;\n      });\n    }, 100000);\n  }\n};",{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyHA;AACA;AAEA;EACAA,YADA;EAEAC,IAFA,kBAEA;IACA;MACAC;QACAC,WADA;QAEAC,QAFA;QAGAC;UACAC,gBADA;UAEAC,YAFA;UAGAC,aAHA;UAIAC,YAJA;UAKAC;QALA,CAHA;QAUAC;UACAC,YADA;UAEAC,WAFA;UAGAC;QAHA,CAVA;QAeAC;UACAC,UADA;UAEAC,OAFA;UAGAC,MAHA;UAIAP;QAJA;MAfA,CADA;MAuBAQ;QACAC,gBADA;QAEAC,mBAFA;QAGAC,gBAHA;QAIAC;MAJA;IAvBA;EA8BA,CAjCA;EAkCAC,WAlCA;EAqCAC;IACAC;EADA,CArCA;EAwCAC;IAAA;;IACAC;MACA;IACA,CAFA;IAGAA,uDACA;MACA;IACA,CAHA;IAIAC;MACAD,wDACA;QACA;MACA,CAHA;IAIA,CALA,EAKA,MALA;EAQA;AAxDA","names":["name","data","tj_datas","notices","news","overview","project_count","api_count","case_count","env_count","user_count","monitor","case_pass","api_pass","case_fail","contribution","project","case","api","real_time_datas","ApiShop_count","UnReadNews_count","RunCase_count","Import_count","methods","components","Menu","mounted","axios","setInterval"],"sourceRoot":"src/views","sources":["Home.vue"],"sourcesContent":["<template>\n  <div id=\"home\" style=\"height: 100%\">\n   <el-container style=\"height: 100%\">\n      <el-aside width=\"200px\">\n            <Menu></Menu>\n      </el-aside>\n      <el-container>\n        <el-header>\n            <el-row :gutter=\"5\" style='margin-top: 5px'>\n                <el-col :span=\"6\">\n                    <div class=\"staticbanner\" style=\"background-color: #3a8ee6\">\n                        <div class=\"title\">\n                            <p>官方接口: {{ real_time_datas.ApiShop_count }}</p>\n                            <el-tag size=\"mini\">实时</el-tag>\n                        </div>\n                    </div>\n                </el-col>\n\n                <el-col :span=\"6\">\n                    <div class=\"staticbanner\" style=\"background-color: #e6a703\">\n                        <div class=\"title\">\n                            <p>未读消息: {{ real_time_datas.UnReadNews_count }}</p>\n                            <el-tag size=\"mini\" style=\"color: #e6a703\">实时</el-tag>\n                        </div>\n                    </div>\n                </el-col>\n\n                <el-col :span=\"6\">\n                    <div class=\"staticbanner\" style=\"background-color: #02b426\">\n                        <div class=\"title\">\n                            <p>用例执行次数: {{ real_time_datas.RunCase_count }}</p>\n                            <el-tag size=\"mini\" style=\"color: #02b426\">实时</el-tag>\n                        </div>\n                    </div>\n                </el-col>\n\n                <el-col :span=\"6\">\n                    <div class=\"staticbanner\" style=\"background-color: #747474\">\n                        <div class=\"title\">\n                            <p>导入接口次数: {{ real_time_datas.Import_count }}</p>\n                            <el-tag size=\"mini\" style=\"color: #747474\">实时</el-tag>\n                        </div>\n                    </div>\n                </el-col>\n            </el-row>\n\n        </el-header>\n        <el-main>\n            <el-card class=\"box-card\" style=\";width: 30%;float: left;margin-right: 1px\">\n              <div slot=\"header\" class=\"clearfix\">\n                <span>项目总览</span>\n              </div>\n                <p>项目总数：{{tj_datas.overview.project_count}}</p>\n                <p>接口总数：{{tj_datas.overview.api_count}}</p>\n                <p>用例总数：{{tj_datas.overview.case_count}}</p>\n                <p>环境总数：{{tj_datas.overview.env_count}}</p>\n                <p>用户总数：{{tj_datas.overview.user_count}}</p>\n            </el-card>\n\n            <el-card class=\"box-card\" style=\"width:-webkit-calc(70% - 20px)\">\n              <div slot=\"header\" class=\"clearfix\">\n                <span>上次监控情况</span>\n              </div>\n                <span style=\"font-size: xx-small\">用例通过率</span>\n                <el-progress style=\"margin-bottom: 10px;\" :text-inside=\"true\" :stroke-width=\"15\" :percentage=\"tj_datas.monitor.case_pass\"></el-progress>\n                <span  style=\"font-size: xx-small\">接口通过率</span>\n                <el-progress style=\"margin-bottom: 10px\" :text-inside=\"true\" :stroke-width=\"15\" :percentage=\"tj_datas.monitor.api_pass\" status=\"success\"></el-progress>\n                <span  style=\"font-size: xx-small\">用例失败/报错率</span>\n                <el-progress style=\"margin-bottom: 10px\" :text-inside=\"true\" :stroke-width=\"15\" :percentage=\"tj_datas.monitor.case_fail\" status=\"exception\"></el-progress>\n            </el-card>\n\n\n             <el-card class=\"box-card\" style=\"float: left;width:-webkit-calc(70% - 20px);margin-right: 1px\">\n              <div slot=\"header\" class=\"clearfix\">\n                <span>个人贡献</span>\n              </div>\n                 <table class=\"table\">\n                     <thead>\n                        <tr>\n                            <th>个人项目占比</th>\n                            <th>个人用例占比</th>\n                            <th>个人接口占比</th>\n                            <th>监控贡献占比</th>\n                        </tr>\n                     </thead>\n                     <tbody>\n                        <tr>\n                            <td><el-progress type=\"circle\" :percentage=\"tj_datas.contribution.project\"></el-progress></td>\n                            <td><el-progress type=\"circle\" :percentage=\"tj_datas.contribution.case\"  color=\"#2c3e50\"></el-progress></td>\n                            <td><el-progress type=\"circle\" :percentage=\"tj_datas.contribution.api\" color=\"green\"></el-progress></td>\n                            <td><el-progress type=\"circle\" :percentage=\"tj_datas.contribution.monitor\" color=\"indianred\"></el-progress></td>\n                        </tr>\n                     </tbody>\n                 </table>\n            </el-card>\n\n            <el-card class=\"box-card\" style=\"overflow-y: auto;width: 30%;\">\n              <div slot=\"header\" class=\"clearfix\">\n                <span>待处理消息</span>\n                  <a href=\"#/send_news/\"><el-button style=\"float: right; padding: 3px 0\" type=\"text\">进入详情</el-button></a>\n              </div>\n                <div v-for=\"o in tj_datas.news\" class=\"text item\">\n                    {{o.from_user_name+' 的消息： ' + o.content }}\n                  </div>\n            </el-card>\n\n        </el-main>\n        <el-footer style=\"min-height: 110px;padding-top: 15px;text-align: left\">\n            <el-card class=\"box-card\" style=\"overflow-y: auto;width: 100%;min-height: 90%;\">\n              <div v-for=\"o in tj_datas.notices\"  class=\"text item\">\n                {{'平台更新日志： ' + o.content }}\n              </div>\n            </el-card>\n        </el-footer>\n      </el-container>\n    </el-container>\n\n  </div>\n</template>\n\n<script>\n    import Menu from '../components/Menu.vue'\n    import axios from 'axios'\n\n    export default {\n      name: 'home',\n      data() {\n          return {\n            tj_datas:{\n                notices:[],\n                news:[],\n                overview:{\n                    project_count:0,\n                    api_count:0,\n                    case_count:0,\n                    env_count:0,\n                    user_count:0\n                },\n                monitor:{\n                    case_pass:0,\n                    api_pass:0,\n                    case_fail:0\n                },\n                contribution:{\n                    project:0,\n                    case:0,\n                    api:0,\n                    monitor:0\n                }\n            },\n            real_time_datas:{\n                ApiShop_count:0,\n                UnReadNews_count:0,\n                RunCase_count:0,\n                Import_count:0,\n            }\n          };\n        },\n      methods:{\n\n      },\n      components: {\n          Menu,\n      },\n      mounted:function () {\n        axios(this.baseUrl+'/get_tj_datas/').then(res=>{\n           this.tj_datas = res.data;\n        });\n        axios.get(this.baseUrl+'/get_real_time_datas/').then(\n            res=>{\n                this.real_time_datas = res.data\n            });\n        setInterval(()=>{\n            axios.get(this.baseUrl+'/get_real_time_datas/').then(\n                res=>{\n                    this.real_time_datas = res.data\n                }\n            )},100000\n        )\n\n      }\n    }\n</script>\n\n<style>\n   .el-header, .el-footer {\n    background:linear-gradient(to right, #d7f7ff, #ffedfd);\n    color: #333;\n    text-align: center;\n  }\n\n  .el-aside {\n    background-color: #D3DCE6;\n    color: #333;\n    text-align: center;\n\n  }\n\n  .el-main {\n    background-color: white;\n    color: #333;\n  }\n  .el-card{\n      background-color: white;\n      text-align: left;\n      overflow-y: auto;\n  }\n\n  .el-container:nth-child(5) .el-aside,\n  .el-container:nth-child(6) .el-aside {\n    line-height: 260px;\n  }\n\n  .el-container:nth-child(7) .el-aside {\n    line-height: 320px;\n  }\n\n  .text {\n    font-size: 14px;\n  }\n\n  .item {\n    margin-bottom: 0;\n  }\n\n  .clearfix:before,\n  .clearfix:after {\n    display: table;\n    content: \"\";\n  }\n  .clearfix:after {\n    clear: both\n  }\n    .box-card{\n        max-height: 49%;\n        min-height: 49%;\n    }\n    th,td{\n        text-align: center;\n        font-size: xx-small;\n        width: 20%;\n    }\n    .staticbanner{\n        color: white;\n        height: 50px;\n        border-radius: 3px;\n        padding: 0 8px;\n    }\n    .title{\n        display: flex;\n        justify-content: space-between;\n        align-items: center;\n    }\n    p{\n        font-size: 13px;\n        font-weight: bold;\n    }\n</style>"]}]}