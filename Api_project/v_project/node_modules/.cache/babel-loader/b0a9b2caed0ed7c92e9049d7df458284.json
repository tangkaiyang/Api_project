n(res=>{\n                    this.all_reports = []\n                })\n            },\n             look_report_detail(index){\n                this.apis_result = [];\n                this.dialogFormVisible3 = true;\n                this.apis_result = this.all_reports[index].apis_result;\n                this.now_report_id = this.all_reports[index].id\n                console.log(this.apis_result)\n            },\n             getColor(result){\n                if( result.toString() == 'true'){\n                    return 'green'\n                }else{\n                    return 'red'\n                }\n            },\n\n        },\n        props:['project_id',]\n    }\n</script>\n\n<style scoped>\n\n</style>"]}]}